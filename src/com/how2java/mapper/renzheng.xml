<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="com.how2java.mapper.renzhengMapper">
<insert id="insertOne" parameterType="renzheng">
INSERT INTO renzheng(username,schoolid,img1,img2,img3)VALUES(#{username},#{schoolid},#{img1},#{img2},#{img3})
</insert>
     <select id="selectByid" resultType="renzheng">
     select * from renzheng where schoolid=#{0}
     </select>
     <delete id="deleteonebyusername">
     delete from renzheng where username like #{0}
     </delete>
     
	</mapper>